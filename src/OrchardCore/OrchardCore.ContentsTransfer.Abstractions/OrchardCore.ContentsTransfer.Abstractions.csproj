<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>OrchardCore.ContentTransfer</RootNamespace>
    <!-- NuGet properties-->
    <Title>OrchardCore ContentTransfer Abstractions</Title>
    <Description>
      $(OCCMSDescription)

      Abstractions for ContentTransfer Module
    </Description>
    <PackageTags>$(PackageTags) OrchardCoreCMS Abstractions</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
